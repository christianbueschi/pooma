{
  "name": "pooma",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "run-s tab:dev tab:serveo open:page",
    "dev": "NODE_ENV=development parcel index.html",
    "serveo": "ssh -R pooma:80:localhost:1234 serveo.net",
    "build": "NODE_ENV=production parcel build index.html --no-minify --public-url ./",
    "deploy": "NODE_ENV=production parcel build index.html --no-minify --public-url ./ && firebase deploy",
    "lint": "./node_modules/.bin/eslint .",
    "tab:dev": "ttab -a iTerm2 -t 'Dev' npm run dev",
    "tab:serveo": "ttab -a iTerm2 -t 'serveo' npm run serveo",
    "open:page": "open 'http://localhost:1234'"
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "description": "",
  "dependencies": {
    "@firebase/app": "0.6.3",
    "@firebase/auth": "0.14.5",
    "@firebase/firestore": "1.14.3",
    "babel-eslint": "10.1.0",
    "chart.js": "2.9.3",
    "firebase": "7.14.3",
    "store": "2.0.12",
    "vue": "2.6.11",
    "vue-chartjs": "3.5.0",
    "vue-hot-reload-api": "2.3.4",
    "vue-router": "3.1.6",
    "vue-styled-components": "1.5.1",
    "vuex": "3.4.0",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.2",
    "babel-core": "6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "sass": "^1.26.5",
    "stylelint": "13.3.3",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.17.2",
    "ttab": "0.6.1",
    "vue-template-compiler": "^2.6.11"
  }
}
